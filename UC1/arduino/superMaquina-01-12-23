/**
SuperMaquina
@author Denis D'Angelo
*/
int tempo = 75;
void setup() {
  pinMode(2, OUTPUT);
  pinMode(3, OUTPUT);
  pinMode(4, OUTPUT);
  pinMode(5, OUTPUT);
  pinMode(6, OUTPUT);
  pinMode(7, OUTPUT);
}

void loop() {
  digitalWrite(2, HIGH);
  delay(tempo);
  digitalWrite(2, LOW);
  digitalWrite(3, HIGH);
  delay(tempo);
  digitalWrite(3, LOW);
  digitalWrite (4, HIGH);
  delay(tempo);
  digitalWrite(4,LOW);
  digitalWrite (5, HIGH);
  delay(tempo);
  digitalWrite(5,LOW);
  digitalWrite (6, HIGH);
  delay(tempo);
  digitalWrite(6,LOW);
  digitalWrite (7, HIGH);
  delay(tempo);
  digitalWrite(7,LOW);
  digitalWrite (6, HIGH);
  delay(tempo);
  digitalWrite(6,LOW);
  digitalWrite (5, HIGH);
  delay(tempo);
  digitalWrite(5,LOW);
  digitalWrite (4, HIGH);
  delay(tempo);
  digitalWrite(4,LOW);
  digitalWrite (3, HIGH);
  delay(tempo);
  digitalWrite(3,LOW);
  
}
